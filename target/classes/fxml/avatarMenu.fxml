<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<Pane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="AvatarMenu"
            prefHeight="720.0" prefWidth="1280.0" stylesheets="@../styles/style.css" styleClass="avatarMenu">
    <Button layoutX="1200" fx:id="mute" text="mute" styleClass="mute" onMouseClicked="#mute"/>
    <Label layoutX="340" layoutY="40" styleClass="Title" text="avatar menu" />
    <Button layoutX="400" layoutY="400" styleClass="Button" text="blue" onMouseClicked="#blueChosen"/>
    <ImageView layoutX="400" layoutY="265" styleClass="blueAvatar"/>
    <Button layoutX="750" layoutY="400" styleClass="Button" text="red" onMouseClicked="#redChosen"/>
    <ImageView layoutX="750" layoutY="265" styleClass="redAvatar"/>
</Pane>
